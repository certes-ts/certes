{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "format": {},
    "lint": {},
    "bench": {},
    "test": {
      "dependsOn": ["^build"],
      "outputs": ["coverage"]
    },
    "test:types": {
      "dependsOn": ["^build"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist", "build"]
    }
  }
}
