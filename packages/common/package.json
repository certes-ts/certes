{
  "$schema": "https://json.schemastore.org/package",
  "name": "@certes/common",
  "title": "Certes Common",
  "description": "A set of common baseline functions in JavaScript.",
  "author": "https://github.com/certes-ts",
  "keywords": [],
  "subPath": "packages/common",
  "version": "0.1.2",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/certes-ts/certes"
  },
  "bugs": "https://github.com/certes-ts/certes/issues",
  "type": "module",
  "files": [
    "./dist/**",
    "./catalog-info.yaml",
    "./CHANGELOG.md",
    "./package.json",
    "./README.md"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./lookup": "./dist/lookup/index.js",
    "./noop": "./dist/noop/index.js",
    "./once": "./dist/once/index.js",
    "./tap": "./dist/tap/index.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "bench": "pnpm vitest bench --run",
    "build": "pnpm tsdown",
    "format": "pnpm biome check --write",
    "lint": "pnpm biome lint",
    "test": "pnpm vitest --dir=src --typecheck"
  },
  "dependencies": {
    "@certes/combinator": "workspace:*"
  },
  "devDependencies": {
    "@vitest/coverage-istanbul": "^4.0.8",
    "tsdown": "^0.17.2",
    "typescript": "5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.8"
  },
  "engines": {
    "node": "22.x",
    "pnpm": "10.x"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
