{
  "$schema": "https://json.schemastore.org/package",
  "name": "@certes/list",
  "title": "Certes List",
  "description": "A set of baseline functions in JavaScript.",
  "author": "https://github.com/certes-ts",
  "keywords": [],
  "subPath": "packages/list",
  "version": "0.1.1",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/certes-ts/certes"
  },
  "bugs": "https://github.com/certes-ts/certes/issues",
  "type": "module",
  "files": [
    "./dist/**",
    "./catalog-info.yaml",
    "./CHANGELOG.md",
    "./package.json",
    "./README.md"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./chunk": "./dist/chunk/index.js",
    "./concat": "./dist/concat/index.js",
    "./every": "./dist/every/index.js",
    "./filter": "./dist/filter/index.js",
    "./find": "./dist/find/index.js",
    "./find-index": "./dist/find-index/index.js",
    "./find-last": "./dist/find-last/index.js",
    "./find-last-index": "./dist/find-last-index/index.js",
    "./flat-map": "./dist/flat-map/index.js",
    "./flatten": "./dist/flatten/index.js",
    "./includes": "./dist/includes/index.js",
    "./index-of": "./dist/index-of/index.js",
    "./map": "./dist/map/index.js",
    "./push": "./dist/push/index.js",
    "./reduce": "./dist/reduce/index.js",
    "./reduce-right": "./dist/reduce-right/index.js",
    "./reverse": "./dist/reverse/index.js",
    "./scan": "./dist/scan/index.js",
    "./shift": "./dist/shift/index.js",
    "./slice": "./dist/slice/index.js",
    "./some": "./dist/some/index.js",
    "./types": "./dist/types.js",
    "./unique": "./dist/unique/index.js",
    "./unique-by": "./dist/unique-by/index.js",
    "./unshift": "./dist/unshift/index.js",
    "./zip": "./dist/zip/index.js",
    "./zip-with": "./dist/zip-with/index.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "bench": "pnpm vitest bench --run",
    "build": "pnpm tsdown",
    "format": "pnpm biome check --write",
    "lint": "pnpm biome lint",
    "test": "pnpm vitest --dir=src",
    "test:types": "pnpm vitest --typecheck"
  },
  "devDependencies": {
    "@vitest/coverage-istanbul": "^4.0.8",
    "tsdown": "^0.17.2",
    "typescript": "5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.8"
  },
  "engines": {
    "node": "22.x",
    "pnpm": "10.x"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
